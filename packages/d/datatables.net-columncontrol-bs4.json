{
  "name": "datatables.net-columncontrol-bs4",
  "description": "Bootstrap 4 styling for DataTables ColumnControl",
  "keywords": [
    "DataTables",
    "table",
    "Bootstrap"
  ],
  "license": "MIT",
  "homepage": "https://datatables.net/extensions/columncontrol",
